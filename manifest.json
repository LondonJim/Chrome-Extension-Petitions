{
  "manifest_version": 2,
  "name": "Random Petitions",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "./ext/icons/icon16.png",
    "32": "./ext/icons/icon32.png",
    "48": "./ext/icons/icon48.png",
    "128": "./ext/icons/icon128.png"
  },
  "browser_action": {
    "default_popup": "./ext/petitions.html"
  }
}
